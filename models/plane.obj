# Model for rendering a single square which covers the entire frame
# Projection matrix should be set glm::ortho(-0.5, 0.5, -0.5, 0.5, -0.5, 0.5);
# View matrix should be set glm::lookAt(glm::vec3(0), glm::vec3(0,-1,0), glm::vec3(0,0,1));
v 0.5 0 -0.5
v -0.5 0 -0.5
v -0.5 0 0.5
v 0.5 0 0.5
vn 0 1 0
f 1//1 2//1 3//1
f 1//1 3//1 4//1

